.tinypass-header-logo {
    background: url("../images/tinypass.png") no-repeat left center;
    height: 70px;
    width: 100%;
}

.tinypass-header-logo-go {
    background: url("../images/tinypassGO.png") no-repeat left center;
}

.tinypass-header-logo-vx {
    background: url("../images/tinypassVX.png") no-repeat left center;
}

#tinypass_metabox h3.hndle span {
    color: transparent;
    background: url("../images/tinypass.png");
    background-size: contain;
    width: 50%;
    display: inline-block;
    background-repeat: no-repeat;
}

.nav-tab-wrapper .nav-tab.nav-tab-active.tinypass-nav-tab-active{
    background-color: #ffffff;
}

.postbox.tinypass-tabbed-postbox{
    border-top: 1px solid transparent;
}
/**
 * RESOURCES EDITOR
 */

.tinypass-resource-editor .inside {
    padding: 0 30px 30px;
}

.tinypass-resource-table {
    width: 100%;
    color: #c5c5c5;
}

.tinypass-resource-table td {
    vertical-align: top;
}

.tinypass-resource-table a {
    white-space: nowrap;
}

.tinypass-resource-table h3, .tinypass-resource-table p {
    margin: 0;
}

.tinypass-resource-table .dashicons {
    color: #c1c1c1;
    font-size: 25px;
}

.tinypass-resource-table tr td.tinypass-resource-image-cell {
    padding: 20px 20px 0 0;
}

.tinypass-resource-table tr td.tinypass-resource-description-cell {
    padding-top: 20px;
}

.tinypass-resource-table tr td.tinypass-resource-description-cell h3 {
    font-size: 14px;
}

.tinypass-resource-table tr td.tinypass-resource-description-cell p {
    font-size: 13px;
}

.tinypass-resource-table tr:first-of-type td {
    vertical-align: middle;
    height: 60px;
}

.tinypass-resource-table tr:first-of-type h3 {
    font-weight: normal;
    color: #212121;
}

.tinypass-resource-table tr.tinypass-terms-separator-row td {
    padding: 0;
}

.tinypass-resource-table tr.tinypass-terms-separator-row td.tinypass-terms-separator-cell {
    background-image: url(../images/px.png);
    background-repeat: repeat-x;
    background-position: 50% 50%;
}

.tinypass-resource-table tr.tinypass-terms-separator-row td.tinypass-terms-separator-cell span {
    background-color: #ffffff;
    font-size: 11px;
    text-transform: uppercase;
    padding-right: 10px;
}

.tinypass-resource-image-cell * {
    width: 80px;
    height: 80px;
    background-color: #f1f1f1;
}

.tinypass-resource-row td, .tinypass-resource-row th {
    border-top: 1px #f1f1f1 solid;
}

.tinypass-resource-table tr.tinypass-resource-row-noterms td.tinypass-resource-image-cell {
    padding-bottom: 20px;
}

/*.tinypass-resource-row .tinypass-resource-buttons-cell:first-of-type{*/
/*border-left: 1px #f1f1f1 solid;*/
/*}*/
.tinypass-resource-table tr.tinypass-term-row td {
    padding-top: 15px;
    padding-bottom: 15px;
}

.tinypass-resource-table tr.tinypass-term-row strong {
    color: #000000;
}

.tinypass-resource-table td.tinypass-new-term-button-cell {
    padding: 0 35px;
    border-right: 1px #f1f1f1 solid;
    vertical-align: middle;
}

a.tinypass-button {
    display: inline-block;
    padding: 6px 18px;
    font-weight: 100;
    background: rgb(89, 146, 224); /* Old browsers */
    background: -moz-linear-gradient(top, rgba(89, 146, 224, 1) 0%, rgba(56, 120, 216, 1) 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, rgba(89, 146, 224, 1)), color-stop(100%, rgba(56, 120, 216, 1))); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top, rgba(89, 146, 224, 1) 0%, rgba(56, 120, 216, 1) 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top, rgba(89, 146, 224, 1) 0%, rgba(56, 120, 216, 1) 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(top, rgba(89, 146, 224, 1) 0%, rgba(56, 120, 216, 1) 100%); /* IE10+ */
    background: linear-gradient(to bottom, rgba(89, 146, 224, 1) 0%, rgba(56, 120, 216, 1) 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#5992e0', endColorstr='#3878d8', GradientType=0); /* IE6-9 */
    border-radius: 4px;
    color: #FFFFFF;
    white-space: nowrap;
    text-decoration: none;
    text-align: center;
}

a.tinypass-button:hover {
    background: rgb(89, 146, 224); /* Old browsers */
    background: -moz-linear-gradient(top, rgba(89, 146, 224, .8) 0%, rgba(56, 120, 216, .8) 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, rgba(89, 146, 224, .8)), color-stop(100%, rgba(56, 120, 216, .8))); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top, rgba(89, 146, 224, .8) 0%, rgba(56, 120, 216, .8) 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top, rgba(89, 146, 224, .8) 0%, rgba(56, 120, 216, .8) 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(top, rgba(89, 146, 224, .8) 0%, rgba(56, 120, 216, .8) 100%); /* IE10+ */
    background: linear-gradient(to bottom, rgba(89, 146, 224, .8) 0%, rgba(56, 120, 216, .8) 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#5992e0', endColorstr='#3878d8', GradientType=0); /* IE6-9 */
}

a.tinypass-button-cancel {
    border: 1px solid #f1f1f1;
    background: #ffffff;
    color: #c1c1c1;
}

a.tinypass-button-cancel:hover {
    background: #f1f1f1;
}

.tinypass-loading:after {
    content: "";
    width: 100%;
    height: 100%;
    background-color: rgba(255, 255, 255, .8);
    background-image: url("../images/wait.gif");
    background-repeat: no-repeat;
    background-position: center center;
    position: absolute;
    top: 0;
    left: 0;
}

.tinypass-synchronize {

}

