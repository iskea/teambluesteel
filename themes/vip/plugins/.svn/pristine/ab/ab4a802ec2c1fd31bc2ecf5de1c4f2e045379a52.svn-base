/**
* POPUP
**/
.tinypass-popup-header {
    background-color: #f8f8f8;
    border-bottom: 1px #f1f1f1 solid;
    width: 100%;
    position: absolute;
    left: 0;
    top: 0;
    margin: 0;
    padding: 17px 0;
    height: 18px;
}

.tinypass-popup-button-container {
    position: absolute;
    left: 0;
    bottom: 0;
    height: 40px;
    padding: 30px 0;
    border-top: 1px #f1f1f1 solid;
    width: 100%;
}

.tinypass-popup-content {
    margin-top: 60px;
}

.tinypass-popup-content input, .tinypass-popup-content textarea, .tinypass-popup-content select {
    border-radius: 4px;
    border: 1px solid #f1f1f1;
    margin: 10px 0;
}

.tinypass-popup-content input, .tinypass-popup-content select {
    height: 40px;
    width: 100%;
}

.tinypass-popup-content textarea {
    width: 100%;
    resize: none;
    height: 120px;
}

.tinypass-popup-button-container .tinypass-button {
    padding: 12px 18px;
    width: 74px;
    float: right;
    margin-right: 20px;
}

.tinypass-popup-header span {
    font-size: 14px;
    margin: 20px;
}

.tinypass-popup-ter-pricing-table span {
    color: #7e7e7e;
    font-size: 14px;
}

.tinypass-term-price-currency-label {
    float: right;
    position: relative;
    right: 10px;
    margin-top: -39px;
}